<h1>Users#index</h1>
<table border= 1>
<tr> 
      <td><%= :Name%></td>
      <td><%= :Body%></td>
       <td><%= :Show%></td>
        <td><%= :Edit%></td>
         <td><%= :Delete%></td>
</tr>
      <%@users.each do |f|%>
         <tr>
         	<td><%= f.name%></td>
         	<td><%= f.body%></td>
         	<td><%= link_to "show", user_path(id: f.id)%></td>
         	<td><%= link_to "edit", edit_user_path(f.id)%></td>
         	<td><%= button_to "delete",user_path(f.id), method: :delete, data: {confirm: "Are you sure"}%></td>
         </tr> 

<%end%>
</table>


<%= link_to "Add new User",new_user_path%>